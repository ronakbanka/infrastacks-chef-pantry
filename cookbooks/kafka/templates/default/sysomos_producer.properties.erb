# This file is generated by Chef. Changes will be overwritten.

<% if @vayacondios_host.nil? || @vayacondios_host.empty? %>
# Chef unable to discover vayacondios, defaulting to localhost
vayacondios.host=localhost
vayacondios.port=8000
<% else %>
vayacondios.host=<%= @vayacondios_host %>
vayacondios.port=<%= @vayacondios_port %>
<% end %>

# Zookeeper connection string
<% if @zookeeper.nil? || @zookeeper.empty? %>
# Chef unable to discover zookeeper, defaulting to localhost
zk.connect=localhost:2181
<% else %>
zk.connect=<%= @zookeeper %>
<% end %>
